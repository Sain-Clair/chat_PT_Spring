<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chun.springpt.Trainer.dao.TrainerDao">
    <select id = "selectList" resultType = 'tvo'>
        select * from members inner join trainer_mem on members.id = trainer_mem.trainer_id order by name desc
    </select>
</mapper>

