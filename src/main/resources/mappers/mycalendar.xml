<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- 왼쪽 구문(http://mybaits) 강제로 다운로드 -->
<mapper namespace="com.chun.springpt.dao.MyCalendarDao">
    <select id="selectCalendarList" resultType="mycalendar">
        select * from mycalendar where userid = #{userid}
    </select>
        

 <select id="getUserReal" resultType="string" parameterType="int">
    SELECT userid FROM mycalendar WHERE eventNum = #{num}
</select>

    <select id="selectNutritionDataByUserId" resultType="nto">
        SELECT 
            p.UPPHOTOPATH,
            f.FOOD_TAN,
            f.FOOD_DAN,
            f.FOOD_GI
        FROM MyCalendar c
        JOIN UPPHOTO p ON c.userid = p.USERID
        JOIN FOOD f ON p.FOODNAME = f.FOODNAME
        WHERE c.userid = #{userid}
    </select>

<delete id="delMyCalendar" parameterType="int">
  DELETE FROM MyCalendar WHERE eventNum = #{num}
</delete>





</mapper>